.bottom
    background-color: #5c5f63;
    width 100%
    height 65px
    padding: 12px 0
    color: #9a9a9a;
    p
        text-align: center
.border_info
    margin-top: 24px
    table
        border-collapse separate
        display table
        width 100%
        text-align: center
        border-radius: 8px 
        border: 1px solid  #ccc
        thead
            line-height: 50px
            text-align: center
            border-bottom: 1px solid  #ccc
            th:first-child
                border-top-left-radius: 12px;
        tbody
            tr:nth-child(odd)
                background:#F4F4F4
                td
                    line-height: 18px
        tr td
                border-bottom: 1px solid  #ccc
                border-right: 1px solid  #ccc
            
