.public {
  .top {
    padding: 16px 0;
    background: #f5f5f5;
  }
  .pull-right {
    padding: 0;
  }
  ul {
    height: 1px;
    margin: 0;
    padding: 0;
    width: 100%;
    li {
      position: relative;
      float: left;
      list-style: none;
      width: 16.6666%;
      line-height: 45px;
      text-align: center;
      a {
        display: inline-block;
        text-decoration: none;
        color: #999999;
        width: 100%;
      }
      .active {
        border-bottom: 2px solid #009eff;
      }
    }
    .xian_i {
      background: #009eff;
      color: #fff;
      border-radius: 6px;
    }
  }
}
